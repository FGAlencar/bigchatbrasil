<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="criacao-tabela-usuario" author="felipe.alencar">
        <preConditions onFail="MARK_RAN" onFailMessage="Tabela usuario jÃ¡ criada">
            <not>
                <tableExists tableName="usuario"/>
            </not>
        </preConditions>
        <createTable tableName="usuario">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="pessoa" type="BIGINT">
                <constraints foreignKeyName="FK_usuario_pessoa" referencedTableName="pessoa" referencedColumnNames="id"/>
            </column>
        </createTable>
        <createTable tableName="usuario_planocontratado">
            <column name="usuario" type="BIGINT"/>
            <column name="planocontratado" type="BIGINT"/>
        </createTable>
    </changeSet>

</databaseChangeLog>